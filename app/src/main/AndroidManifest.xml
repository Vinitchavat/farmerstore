<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kongla.storeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_home" />
        <activity
            android:name=".MarListFruit" />
        <activity
            android:name=".basketMain"
            android:label="@string/title_order" />
        <activity android:name=".basketMandP" />
        <activity android:name=".chatNew" />
        <activity
            android:name=".PreMain"
            android:label="@string/preorder" />
        <activity
            android:name=".PreListDay"
            android:label="@string/pre_daylist"
            android:parentActivityName=".PreMain" />
        <activity android:name=".show" />
        <activity
            android:name=".profileMain"
            android:label="@string/title_profile"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profileEditImage"
            android:label="@string/profile_editimg"
            android:parentActivityName=".profileMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profileEditProfile"
            android:label="@string/profile_edit"
            android:parentActivityName=".profileMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profileEditActivity"
            android:label="@string/profile_edit"
            android:parentActivityName=".profileEditProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profileSellerSetting"
            android:label="@string/profile_sellerSetting"
            android:parentActivityName=".profileMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profileEditFarm"
            android:parentActivityName=".profileMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profileAddProduct"
            android:parentActivityName=".profileMain"
            android:screenOrientation="portrait" />
        <activity android:name=".MarListFruit"></activity>
    </application>

</manifest>